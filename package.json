{
    "name": "mharj-mongoose-cache",
    "version": "0.6.0",
    "description": "mongoose model cache",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "import": "./dist/index.mjs",
            "types": "./dist/index.d.ts"
        }
    },
    "scripts": {
        "test": "mocha",
        "build": "tsup src/index.ts --format cjs,esm --dts --clean",
        "prepublishOnly": "npm t && npm run build",
        "lint": "eslint src",
        "validate": "tsc --noEmit --project tsconfig.test.json"
    },
    "files": [
        "dist"
    ],
    "mocha": {
        "exit": true,
        "extension": [
            "ts",
            "js"
        ],
        "recursive": true,
        "require": [
            "ts-node/register",
            "source-map-support/register"
        ],
        "reporters": [
            "spec",
            "mocha-junit-reporter"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mharj/mongoose-cache.git"
    },
    "keywords": [
        "mongo",
        "mongoose",
        "cache"
    ],
    "author": "mharj",
    "license": "LGPL-2.0-only",
    "bugs": {
        "url": "https://github.com/mharj/mongoose-cache/issues"
    },
    "homepage": "https://github.com/mharj/mongoose-cache#readme",
    "devDependencies": {
        "@stylistic/eslint-plugin": "^2.1.0",
        "@tsconfig/node16": "^16.1.3",
        "@types/chai": "^4.3.16",
        "@types/mocha": "^10.0.6",
        "@types/node": "^18.19.33",
        "@types/sinon": "^17.0.3",
        "@types/timsort": "^0.3.3",
        "@typescript-eslint/eslint-plugin": "^7.10.0",
        "@typescript-eslint/parser": "^7.10.0",
        "chai": "^4.4.1",
        "eslint": "^8.57.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-config-standard": "^17.1.0",
        "eslint-plugin-deprecation": "^2.0.0",
        "eslint-plugin-prettier": "^5.1.3",
        "eslint-plugin-sonarjs": "^0.23.0",
        "mocha": "^10.4.0",
        "mongodb-memory-server": "^9.2.0",
        "mongoose": "^8.4.0",
        "prettier": "^3.2.5",
        "sinon": "^18.0.0",
        "ts-node": "^10.9.2",
        "tslib": "^2.6.2",
        "tsup": "^8.0.2",
        "typescript": "^5.4.5"
    },
    "peerDependencies": {
        "mongoose": "^6.x || ^7.x || ^8.x"
    },
    "dependencies": {
        "@avanio/logger-like": "^0.1.1",
        "typed-emitter": "^2.1.0"
    }
}
