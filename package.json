    "devDependencies": {
        "@types/chai": "^4.3.9",
        "@types/mocha": "^10.0.2",
        "@types/node": "^18.18.5",
        "@types/sinon": "^10.0.19",
        "@types/timsort": "^0.3.1",
        "@typescript-eslint/eslint-plugin": "^6.8.0",
        "@typescript-eslint/parser": "^6.8.0",
        "chai": "^4.3.10",
        "eslint": "^8.51.0",
