{
    "name": "mharj-mongoose-cache",
    "version": "0.9.2",
    "description": "mongoose model cache",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "require": "./dist/index.js",
            "import": "./dist/index.mjs"
        }
    },
    "scripts": {
        "build": "tsup src/index.ts --format cjs,esm --dts --clean",
        "prepublishOnly": "npm run build",
        "test": "vitest test --run --no-isolate --coverage",
        "coverage": "vitest test --run --no-isolate --reporter=dot --coverage --coverage.reporter=lcov",
        "lint": "eslint . --ext .ts",
        "validate": "tsc --noEmit --project tsconfig.test.json"
    },
    "files": [
        "dist"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mharj/mongoose-cache.git"
    },
    "keywords": [
        "mongo",
        "mongoose",
        "cache"
    ],
    "author": "mharj",
    "license": "LGPL-2.0-only",
    "bugs": {
        "url": "https://github.com/mharj/mongoose-cache/issues"
    },
    "homepage": "https://github.com/mharj/mongoose-cache#readme",
    "devDependencies": {
        "@avanio/logger-like": "^0.2.11",
        "@cspell/eslint-plugin": "^8.17.5",
        "@eslint/js": "^9.23.0",
        "@stylistic/eslint-plugin": "^4.2.0",
        "@stylistic/eslint-plugin-ts": "^4.2.0",
        "@tsconfig/node18": "^18.2.4",
        "@types/node": "^22.13.13",
        "@types/sinon": "^17.0.4",
        "@typescript-eslint/eslint-plugin": "^8.28.0",
        "@typescript-eslint/parser": "^8.28.0",
        "@vitest/coverage-v8": "^3.0.9",
        "c8": "^10.1.3",
        "eslint": "^9.23.0",
        "eslint-config-prettier": "^10.1.1",
        "eslint-import-resolver-typescript": "^4.2.3",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-jsdoc": "^50.6.9",
        "eslint-plugin-prettier": "^5.2.5",
        "eslint-plugin-sonarjs": "^3.0.2",
        "mongodb-memory-server": "^10.1.4",
        "mongoose": "^8.13.0",
        "prettier": "^3.5.3",
        "sinon": "^20.0.0",
        "tslib": "^2.8.1",
        "tsup": "^8.4.0",
        "typescript": "^5.8.2",
        "typescript-eslint": "^8.28.0",
        "vite": "^6.2.3",
        "vitest": "^3.0.9"
    },
    "peerDependencies": {
        "@avanio/logger-like": ">= 0.0.1",
        "mongoose": ">= 6.0.0"
    },
    "packageManager": "pnpm@10.6.5+sha512.cdf928fca20832cd59ec53826492b7dc25dc524d4370b6b4adbf65803d32efaa6c1c88147c0ae4e8d579a6c9eec715757b50d4fa35eea179d868eada4ed043af",
    "pnpm": {
        "onlyBuiltDependencies": [
            "esbuild",
            "mongodb-memory-server"
        ]
    }
}