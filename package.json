{
    "name": "mharj-mongoose-cache",
    "version": "0.7.0",
    "description": "mongoose model cache",
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "require": "./dist/index.js",
            "import": "./dist/index.mjs",
            "types": "./dist/index.d.ts"
        }
    },
    "scripts": {
        "test": "mocha",
        "build": "tsup src/index.ts --format cjs,esm --dts --clean",
        "prepublishOnly": "npm t && npm run build",
        "lint": "eslint . --ext .ts",
        "validate": "tsc --noEmit --project tsconfig.test.json"
    },
    "files": [
        "dist"
    ],
    "mocha": {
        "exit": true,
        "extension": [
            "ts",
            "js"
        ],
        "recursive": true,
        "require": [
            "ts-node/register",
            "source-map-support/register"
        ],
        "reporters": [
            "spec",
            "mocha-junit-reporter"
        ]
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/mharj/mongoose-cache.git"
    },
    "keywords": [
        "mongo",
        "mongoose",
        "cache"
    ],
    "author": "mharj",
    "license": "LGPL-2.0-only",
    "bugs": {
        "url": "https://github.com/mharj/mongoose-cache/issues"
    },
    "homepage": "https://github.com/mharj/mongoose-cache#readme",
    "devDependencies": {
        "@avanio/logger-like": "^0.2.5",
        "@stylistic/eslint-plugin": "^2.9.0",
        "@stylistic/eslint-plugin-ts": "^2.9.0",
        "@tsconfig/node16": "^16.1.3",
        "@types/chai": "^4.3.20",
        "@types/mocha": "^10.0.9",
        "@types/node": "^18.19.56",
        "@types/sinon": "^17.0.3",
        "@types/timsort": "^0.3.3",
        "@typescript-eslint/eslint-plugin": "^8.10.0",
        "@typescript-eslint/parser": "^8.10.0",
        "chai": "^4.5.0",
        "eslint": "^8.57.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-config-standard": "^17.1.0",
        "eslint-plugin-deprecation": "^3.0.0",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-sonarjs": "^0.23.0",
        "mocha": "^10.7.3",
        "mongodb-memory-server": "^10.1.2",
        "mongoose": "^8.7.2",
        "prettier": "^3.3.3",
        "sinon": "^19.0.2",
        "ts-node": "^10.9.2",
        "tslib": "^2.8.0",
        "tsup": "^8.3.0",
        "typescript": "^5.6.3"
    },
    "peerDependencies": {
        "@avanio/logger-like": "^0.0 || ^0.1 || ^0.2",
        "mongoose": "^6 || ^7 || ^8"
    }
}